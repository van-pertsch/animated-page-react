  <img src={movie.mainImg} alt=\"movie image\" />\n          </Headline>\n          <Awards>\n            {movie.awards.map((award) => (\n              <Award\n                title={award.title}\n                description={award.description}\n                key={award.title}\n              />\n            ))}\n          </Awards>\n          <ImageDisplay>\n            <img src={movie.secondaryImg} alt=\"\" />\n          </ImageDisplay>\n        </Details>\n      )}\n    </>\n  );\n};\n\nconst Details = styled(motion.div)`\n  color: #000;\n`;\n\nconst Headline = styled.div`\n  min-height: 90vh;\n  padding-top: 20vh;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    text-transform: uppercase;\n    /* font-weight: bold; */\n    transform: translate(-50%, -10%);\n  }\n  .line {\n    height: 0.2rem;\n    background: #3b3b3b;\n    margin-bottom: 3rem;\n    margin-left: 35%;\n    margin-right: 35%;\n  }\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\nconst Awards = styled.div`\n  min-height: 50vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n`;\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 1.8rem;\n    color: #000;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    height: 0.5rem;\n    margin: 1rem 0rem;\n  }\n  p {\n    padding: 2rem 0;\n  }\n`;\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/vanessa/localhost/vanessa/learning/DEVED/react/portfolio-react/src/movieState.js",[],"/Users/vanessa/localhost/vanessa/learning/DEVED/react/portfolio-react/src/animation.js",[],"/Users/vanessa/l